{
  "name": "terraform-quick-docs",
  "displayName": "Terraform Quick Docs",
  "description": "Access Terraform Provider and Function Docs by hovering over Resources.",
  "author": "LemurDaniel",
  "publisher": "LemurDaniel",
  "license": "GPL-3.0",
  "icon": ".resources/simple_logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/LemurDaniel/PROJECT__VSCode_terraform_quick_docs"
  },
  "version": "0.0.12",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "terraform",
    "tf",
    "hcl",
    "devops"
  ],
  "activationEvents": [
    "onLanguage:terraform"
  ],
  "main": "./client/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terraform-quick-docs.resource.show",
        "title": "Terraform Quick Docs: Show Resource Documentation"
      },
      {
        "command": "terraform-quick-docs.functions.show",
        "title": "Terraform Quick Docs: Show Function Documentation"
      },
      {
        "command": "terraform-quick-docs.documentation.show",
        "title": "Terraform Quick Docs: Show additional Documentations"
      },
      {
        "command": "terraform-quick-docs.providers.list",
        "title": "Terraform Quick Docs: Show supported Providers"
      }
    ],
    "languages": [
      {
        "id": "terraform",
        "extensions": [
          ".tf"
        ],
        "aliases": [
          "Terraform"
        ],
        "filenames": []
      }
    ],
    "configuration": {
      "title": "Terraform Quick Docs",
      "properties": {
        "terraform-quick-docs.additional_supported_providers": {
          "type": "array",
          "default": [
            "azure/azapi"
          ],
          "items": {
            "type": "string",
            "pattern": "[a-zA-Z0-9_]/[a-zA-Z0-9_]+",
            "patternErrorMessage": "Must be of format '<namespace>/<provider>'"
          },
          "description": "Add support for other Providers by specifing: '<namespace>/<provider>' like 'azure/azapi'"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint ./client/src ./server/src --ext .js",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "sh ./scripts/e2e.sh"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "mocha": "^9.2.1",
    "path": "^0.12.7",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}